<Window x:Class="ATG_Notifier.Desktop.Views.AppShell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ATG_Notifier.Desktop.Views"
        xmlns:configuration="clr-namespace:ATG_Notifier.Desktop.Configuration"
        x:ClassModifier="internal"
        mc:Ignorable="d"
        Title="{x:Static configuration:AppConfiguration.AppId}" 
        Width="300" Height="400"
        MinWidth="200" MinHeight="300"
        d:DesignHeight="450" d:DesignWidth="800"
        SnapsToDevicePixels="True"
        FontFamily="Segoe UI"
        WindowStartupLocation="Manual"
        Initialized="OnInitialized"
        Loaded="OnLoaded">
    <Grid>
        <Frame x:Name="ContentFrame" 
               IsTabStop="False"/>
    </Grid>
</Window>
